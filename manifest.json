{
    "name": "EBook Reader",
    "description": "EBook Reader for epub and mobi",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
          "matches": [
            "*://*/greenstone3/library/collection/*/document/*"
          ],
          "js": ["gs3-ebookreader.min.js"]
        }
    ]
}